#include <bits/stdc++.h>
using namespace std;

int main() {
  vector<int> n(3);
  for (int i = 0; i < n.size(); i++) {
    cin >> n[i];
  }
  sort(n.begin(), n.end());
  string order;
  cin >> order;
  for (char l: order) {
    if (l == 'A') {
      cout << n[0] << " ";
    } else if (l == 'B') {
      cout << n[1] << " ";
    } else {
      cout << n[2] << " ";
    }
  }
  cout << endl;
  return 0;
}